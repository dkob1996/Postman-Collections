		{
			"name": "Booking Create",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();\r",
							"pm.collectionVariables.set(\"bookingID\",jsonData.bookingid);\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Check first name\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.booking.firstname).to.eql(\"Jim\");\r",
							"});\r",
							"\r",
							"pm.test(\"Check last name\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.booking.lastname).to.eql(\"Boot\");\r",
							"});\r",
							"\r",
							"pm.test(\"Check price\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.booking.totalprice).to.eql(111);\r",
							"});\r",
							"\r",
							"pm.test(\"Check deposit paids\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.booking.depositpaid).to.eql(true);\r",
							"});\r",
							"\r",
							"pm.test(\"Check checkin date\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.booking.bookingdates.checkin).to.eql(\"2018-01-01\");\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
